#include <stdio.h>
#include <math.h>
int main()
{
	printf("Naidem znachenie a^15 ispolzuya 2 vspomogatelnie peremennie i 5 operaziyami umnozheniya: ");
	int a, x, y, rez;
	printf("\nVvedem a: ");
	scanf_s("%i", &a);
	x = pow(a, 4);
	y = pow(a, 2);
	rez = x * x * x * y * a;
	printf("\nRezultat= %i", rez);
}
