#include <stdio.h>
#include <locale.h>
int main() {
	setlocale(LC_ALL, "Rus");
	printf("Вывести все целые числа от A до B (A < B) включительно; при этом каждое число должно выводиться столько раз, каково его значение ");
	int a, b, i, y;
	printf("\Vvedem a: ");
	scanf_s("%i", &a);
	printf("\Vvedem b: ");
	scanf_s("%i", &b);
	for (i = a; i <= b; i++) {
		y = 0;
		while (y < i) {
			printf("%i ", i);
			y++;
		}
	}
	return 0;
}


