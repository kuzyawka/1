#include <stdio.h>
#include <stdlib.h>
int main()
{
	printf("Vivesty datu a-chislo (1-31) i b-mesyaz(1-12) v vide texta: ");
	int a, b;
	printf("\nVvedem a: ");
	scanf_s("%i", &a);
	printf("\nVvedem b: ");
	scanf_s("%i", &b);
	switch (a) {
	case 1:
		printf("pervoe ");
		break;
	case 2:
		printf("vtoroe ");
		break;
