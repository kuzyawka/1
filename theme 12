2.

#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	printf("Вывести направление робота.(«1» — север, «2» — запад, «3» — юг, «4» — восток) и принимать три цифровые команды: 0 — продолжать движение, 1 — поворот налево, −1 — поворот направо. C — исходное направление робота и целое число N — посланная ему команда: ");
	int n,  c;
	printf("\nVvedem с: ");
	scanf_s("%i", &c);
	printf("\nVvedem n: ");
	scanf_s("%i", &n);


switch (c) {
case 1:
	if (n == 0)
		printf("Север");
	if (n == 1)
		printf("Запад");
	if (n == -1)
		printf("Восток");
	break;
case 2:
	if (n == 0)
		printf("Запад");
	if (n == 1)
		printf("Юг");
	if (n == -1)
		printf("Север");
	break;
case 3:
	if (n == 0)
		printf("Юг");
	if (n == 1)
		printf("Восток");
	if (n == -1)
		printf("Запад");
	break;
case 4:
	if(n == 0)
		printf("Восток");
	if (n == 1)
		printf("Север");
	if (n == -1)
		printf("Юг");
	break;

}
return 0;
}




3.



#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	printf("вывести текстом описание количества заданий n(10-40) : ");
	int n; div_t a, b;
	printf("\nVvedem n: ");
	scanf_s("%i", &n);
	a = div(n, 10);
	if ((a.quot >= 2) && (a.rem > 0)) {
		switch (a.quot) {
		case 2:
			printf("двадцать ");
			break;
		case 3:
			printf("тридцать ");
			break;
		case 4:
			printf("сорок ");
			break;

		}
		switch (a.rem) {
		case 1:
			printf("одно ");
			break;
		case 2:
			printf("два ");
			break;
		case 3:
			printf("три ");
			break;
		case 4:
			printf("четыре ");
			break;
		case 5:
			printf("пять ");
			break;
		case 6:
			printf("шесть ");
			break;
		case 7:
			printf("семь ");
			break;
		case 8:
			printf("восемь ");
			break;
		case 9:
			printf("девять ");
			break;

		}
		printf("учебных задания");
	}

	else {
		switch (n) {
		case 10:
			printf("десять ");
			break;
		case 11:
			printf("одиннадцать ");
			break;
		case 12:
			printf("двенадцать ");
			break;
		case 13:
			printf("тринадцать ");
			break;
		case 14:
			printf("четырнадцать ");
			break;
		case 15:
			printf("пятнадцать ");
			break;
		case 16:
			printf("шестнадцать ");
			break;
		case 17:
			printf("семнадцать ");
			break;
		case 18:
			printf("восемнадцать ");
			break;
		case 19:
			printf("девятнадцать ");
			break;
		case 20:
			printf("двадцать ");
			break;
		case 30:
			printf("тридцать ");
			break;
		case 40:
			printf("сорок ");
			break;

		}
		printf("учебных заданий");
	}

return 0;
}



4.




#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	printf("Дано целое число n в диапазоне 100–999. Вывести строку-описание данного числа : ");
	int n; div_t a, b, c;
	printf("\nVvedem n: ");
	scanf_s("%i", &n);
	a = div(n, 10);
	b = div(n, 100);
	c = div((b.rem), 10);
	switch (b.quot) {
	case 1:
		printf("сто ");
		break;
	case 2:
		printf("двести ");
		break;
	case 3:
		printf("триста ");
		break;
	case 4:
		printf("четыреста ");
		break;
	case 5:
		printf("пятьсот ");
		break;
	case 6:
		printf("шестьсот ");
		break;
	case 7:
		printf("семьсот ");
		break;
	case 8:
		printf("восемьсот ");
		break;
	case 9:
		printf("девятьсот ");
		break;
	}
	if ((c.quot >= 2) && (a.rem > 0)) {
		switch (c.quot) {
		case 2:
			printf("двадцать ");
			break;
		case 3:
			printf("тридцать ");
			break;
		case 4:
			printf("сорок ");
			break;
		case 5:
			printf("пятьдесят ");
			break;
		case 6:
			printf("шестьдесят ");
			break;
		case 7:
			printf("семьдесят ");
			break;
		case 8:
			printf("восемьдесят ");
			break;
		case 9:
			printf("девяносто ");
			break;
		}
		switch (a.rem) {
		case 1:
			printf("один ");
			break;
		case 2:
			printf("два ");
			break;
		case 3:
			printf("три ");
			break;
		case 4:
			printf("четыре ");
			break;
		case 5:
			printf("пять ");
			break;
		case 6:
			printf("шесть ");
			break;
		case 7:
			printf("семь ");
			break;
		case 8:
			printf("восемь ");
			break;
		case 9:
			printf("девять ");
			break;

		}
	}
	else {
		switch (b.rem) {
		case 10:
			printf("десять ");
			break;
		case 11:
			printf("одиннадцать ");
			break;
		case 12:
			printf("двенадцать ");
			break;
		case 13:
			printf("тринадцать ");
			break;
		case 14:
			printf("четырнадцать ");
			break;
		case 15:
			printf("пятнадцать ");
			break;
		case 16:
			printf("шестнадцать ");
			break;
		case 17:
			printf("семнадцать ");
			break;
		case 18:
			printf("восемнадцать ");
			break;
		case 19:
			printf("девятнадцать ");
			break;
		case 20:
			printf("двадцать ");
			break;
		case 30:
			printf("тридцать ");
			break;
		case 40:
			printf("сорок ");
			break;
		case 50:
			printf("пятьдесят ");
			break;
		case 60:
			printf("шестьдесят ");
			break;
		case 70:
			printf("семьдесят ");
			break;
		case 80:
			printf("восемьдесят ");
			break;
		case 90:
			printf("девяносто ");
			break;



		}
	}
return 0;
}




5.





#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	printf("По номеру года n определить его название.60-летний цикл, состоящий из 12- летних подциклов, обозначаемых названиями цвета: зеленый, красный, желтый, белый и черный.годы носят названия животных: крысы, коровы, тигра, зайца, дракона, змеи, лошади, овцы, обезьяны, курицы, собаки и свиньи.: ");
	int n; int a, b, c, d;
	printf("\nVvedem n: ");
	scanf_s("%i", &n);
	a = (int)((float)n / 60);
	b = a * 60;
	if (n - b == 0) {
		d = 11;
		c = 4;
	}
	else {
		d= (int)((float)(n - b) / 12);
		c= (int)((n - b) / 5);
	}
	printf(("\nProverka %i  \nProverka %i "), c, d);
	printf("\nгод ");
	if ((d == 3) || (d == 4) || (d == 5)) {
		switch (c) {
		case 0:
			printf("зеленого ");
			break;
		case 1:
			printf("красного ");
			break;
		case 2:
			printf("желтого ");
			break;
		case 3:
			printf("белого ");
			break;
		case 4:
			printf("черного ");
			break;
		}
		switch (d) {
		case 2:
			printf("тигра ");
			break;
		case 3:
			printf("зайца ");
			break;
		case 4:
			printf("дракона ");
			break;
		}
	}
	else {
		switch (c) {
		case 0:
			printf("зеленой ");
			break;
		case 1:
			printf("красной ");
			break;
		case 2:
			printf("желтой ");
			break;
		case 3:
			printf("белой ");
			break;
		case 4:
			printf("черной ");
			break;
		}
		switch (d) {
		case 0:
			printf("крысы ");
			break;
		case 1:
			printf("коровы ");
			break;
		case 5:
			printf("змеи ");
			break;
		case 6:
			printf("лошади ");
			break;
		case 7:
			printf("овцы ");
			break;
		case 8:
			printf("обезьяны ");
			break;
		case 9:
			printf("курицы ");
			break;
		case 10:
			printf("собаки ");
			break;
		case 11:
			printf("свиньи ");
			break;
		}
	}
return 0;
}
