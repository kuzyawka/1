2.

#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	printf("Вывести направление робота.(«1» — север, «2» — запад, «3» — юг, «4» — восток) и принимать три цифровые команды: 0 — продолжать движение, 1 — поворот налево, −1 — поворот направо. C — исходное направление робота и целое число N — посланная ему команда: ");
	int n,  c;
	printf("\nVvedem с: ");
	scanf_s("%i", &c);
	printf("\nVvedem n: ");
	scanf_s("%i", &n);


switch (c) {
case 1:
	if (n == 0)
		printf("Север");
	if (n == 1)
		printf("Запад");
	if (n == -1)
		printf("Восток");
	break;
case 2:
	if (n == 0)
		printf("Запад");
	if (n == 1)
		printf("Юг");
	if (n == -1)
		printf("Север");
	break;
case 3:
	if (n == 0)
		printf("Юг");
	if (n == 1)
		printf("Восток");
	if (n == -1)
		printf("Запад");
	break;
case 4:
	if(n == 0)
		printf("Восток");
	if (n == 1)
		printf("Север");
	if (n == -1)
		printf("Юг");
	break;

}
return 0;
}




3.



#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main()
{
	setlocale(LC_ALL, "Rus");
	printf("вывести текстом описание количества заданий n(10-40) : ");
	int n; div_t a, b;
	printf("\nVvedem n: ");
	scanf_s("%i", &n);
	a = div(n, 10);
	if ((a.quot >= 2) && (a.rem > 0)) {
		switch (a.quot) {
		case 2:
			printf("двадцать ");
			break;
		case 3:
			printf("тридцать ");
			break;
		case 4:
			printf("сорок ");
			break;

		}
		switch (a.rem) {
		case 1:
			printf("одно ");
			break;
		case 2:
			printf("два ");
			break;
		case 3:
			printf("три ");
			break;
		case 4:
			printf("четыре ");
			break;
		case 5:
			printf("пять ");
			break;
		case 6:
			printf("шесть ");
			break;
		case 7:
			printf("семь ");
			break;
		case 8:
			printf("восемь ");
			break;
		case 9:
			printf("девять ");
			break;

		}
		printf("учебных задания");
	}

	else {
		switch (n) {
		case 10:
			printf("десять ");
			break;
		case 11:
			printf("одиннадцать ");
			break;
		case 12:
			printf("двенадцать ");
			break;
		case 13:
			printf("тринадцать ");
			break;
		case 14:
			printf("четырнадцать ");
			break;
		case 15:
			printf("пятнадцать ");
			break;
		case 16:
			printf("шестнадцать ");
			break;
		case 17:
			printf("семнадцать ");
			break;
		case 18:
			printf("восемнадцать ");
			break;
		case 19:
			printf("девятнадцать ");
			break;
		case 20:
			printf("двадцать ");
			break;
		case 30:
			printf("тридцать ");
			break;
		case 40:
			printf("сорок ");
			break;

		}
		printf("учебных заданий");
	}

return 0;
}
